{
  "name": "skillmon",
  "version": "0.10.0",
  "license": "MIT",
  "author": "snipereagle1 <me@snipereagle.xyz>",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "generate-route-tree": "tsr generate",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "typegen": "cargo tauri-typegen generate --output-path src/generated",
    "tauri:dev": "pnpm typegen && tauri dev",
    "tauri:build": "pnpm typegen && tauri build",
    "test": "TZ=UTC vitest",
    "test:watch": "TZ=UTC vitest --watch",
    "test:coverage": "TZ=UTC vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "lint:rust:fix": "cd src-tauri && cargo clippy --fix --allow-dirty --allow-staged",
    "format": "prettier --write . && pnpm format:rust",
    "format:check": "prettier --check .",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:check:rust": "cd src-tauri && cargo fmt -- --check",
    "lint:full": "pnpm lint && pnpm lint:rust",
    "format:check:all": "pnpm format:check && pnpm format:check:rust",
    "verify": "pnpm typegen && pnpm lint:full && pnpm format:check:all && pnpm typecheck",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src-tauri/**/*.rs": [
      "sh -c 'cd src-tauri && cargo clippy -- -D warnings'",
      "sh -c 'cd src-tauri && cargo fmt -- --check'"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.154.6",
    "@tanstack/router-cli": "^1.154.4",
    "@tanstack/router-plugin": "^1.154.6",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "^2",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-resizable-panels": "^2.1.7",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ts-pattern": "^5.9.0",
    "vaul": "^1.1.2",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "shadcn": "^3.7.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
